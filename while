#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
//while语句结构为
//while（表达式）
// 循环语句；超过两句就要加上大括号，最后是一直加上。
//先判断表达式是否为真，如果为真执行语句，执行结束后
// 再次判断表达式是否为真，为真重复以上的步骤，直至表达式
// 为假，循环结束。
//
//
//int main()
//{
//	int i = 1;
//	while (i <= 10)
//	{
//		printf("%d\n", i);
//		i++;
//	}
//
//	return 0;
//}
//int main()
//{
//	int i = 1;
//
//	
//	while (i <= 10)
//
//	{
//		if (4 == i)
//			break;//如果在循环中遇到break，就会停止后期的所有循环，直接结束循环；
// 	   printf("%d\n", i);
//	  i++;
//	}
//	return 0;
//}
//int main()
//{
//	int i = 0;
//	while (i <= 10)
//	{
//		if (4 == i)
//			continue;//如果在循环中遇到continue，则在本次循环中continue后的代码
//		printf("%d\n", i);//不再执行直接跳到while去判断表达式真假真假
//		i++;
//
//	}
//	return 0;
//}
